{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "object-literal-sort-keys": false,
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "no-bitwise": false,
    "no-conditional-assignment": false,
    "no-angle-bracket-type-assertion": false
  },
  "rulesDirectory": []
}
